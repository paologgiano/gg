   "source": [
    "from pylab import *\n",
    "#definisco una funzione h di prova\n",
    "tau=0.05\n",
    "t=arange(-10,10,tau)\n",
    "T=2.\n",
    "h=sin(2*pi*t/T)\n",
    "\n",
    "N=len(h)\n",
    "H=zeros(N,dtype=\"complex\")\n",
    "\n",
    "for n in range(N):\n",
    "    for k in range(N):\n",
    "        H[n] += h[k]*exp(2j*pi*k*n/N)\n",
    "H*=tau\n",
    "xlim(0,50)\n",
    "plot(h)\n",
    "plot(abs(H))"
   ]
